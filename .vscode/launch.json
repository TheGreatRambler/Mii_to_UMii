{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "(gdb) Pc Application Windows",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/bin/miitoumii.exe",
			"args": [
				"-i",
				"3DS_Test_Female.jpg",
				"-o",
				"3DS_Test_Female.umii.yml",
				"-x",
				"-t",
				"3ds"
			],
			"logging": {
				"moduleLoad": true,
				"exceptions": true
			},
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": true,
			"MIMode": "gdb",
			"miDebuggerPath": "gdb.exe",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			]
		},
		{
			"name": "(gdb) Pc Application Linux",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/bin/miitoumii",
			"args": [
				"-i",
				"3DS_Test_Female.cfsd",
				"-o",
				"3DS_Test_Female1.umii.yml",
				"-x",
				"-t",
				"3ds"
			],
			"logging": {
				"moduleLoad": true,
				"exceptions": true
			},
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": true,
			"MIMode": "gdb",
			"miDebuggerPath": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			]
		}
	]
}