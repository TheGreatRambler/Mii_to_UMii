{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "(gdb) Pc Application Windows",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/bin/miitoumii.exe",
			"args": [
				"-i",
				"Npc_HatenoVillage.jpg",
				"-r",
				"C:/Users/aehar/Downloads/romfs",
				"-p",
				"C:/Users/aehar/Downloads/mod",
				"-x"
			],
			"logging": {
				"moduleLoad": true,
				"exceptions": true
			},
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": true,
			"MIMode": "gdb",
			"miDebuggerPath": "gdb.exe",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			]
		},
		{
			"name": "(gdb) Pc Application Linux",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/bin/miitoumii",
			"args": [
				"-i","https://www.miicharacters.com/miis/qr_thumb/1972_peterg.jpg","-b","-x"
			],
			"logging": {
				"moduleLoad": true,
				"exceptions": true
			},
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": true,
			"MIMode": "gdb",
			"miDebuggerPath": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			]
		}
	]
}